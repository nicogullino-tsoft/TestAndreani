{
  "name": "devscore-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run lint && webpack-dev-server --progress --profile --colors",
    "prod": "cd ./public && ws --port 9999 --spa index.html",
    "build": "npm run lint_prod && npm run _build_webpack_prod",
    "lint": "tslint --project tsconfig.json",
    "lint_prod": "tslint --project tsconfig.production.json",
    "test": "export NODE_ENV=test && jest",
    "_tsc_desa": "tsc -p tsconfig.json",
    "_tsc_prod": "tsc -p tsconfig.production.json",
    "_build_webpack_prod": "webpack --config webpack.production.config.js --progress --profile --colors",
    "prepush": "npm run test",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "help-commit-msg": "echo 'example: build, fix, chore, style, refactor, docs, revert, test, feat: message here.'",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/marina.toranzo/devscore.frontend.git"
  },
  "author": "Marina Toranzo",
